(5 > 2) && false -- false

!("knife" === "sword") -- true

(1 < 2) || (-1 > -1) || !false -- true

(31 % 5) == "1" -- false

!!true -- true

"5th Avenue" != "5th Avenue" -- false

52 !== "52" -- true

(undefined || null) -- null


// 2 //

let a = 3, let c = 0, let b = a (Now: a=3, b=3, c=0)

b = a (b stays 3)

c = a (c becomes 3)

b = c (b stays 3)

a = b (a stays 3)